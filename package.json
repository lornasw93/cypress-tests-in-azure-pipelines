{
  "name": "react-vite-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "start:e2e": "cross-env VITE_EHR_SERVER_URL=/ npm run build && npm run serve",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "npm run test:unit && npm run test:component",
    "test:e2e": "cypress run -q",
    "test:component": "cypress run -q --component",
    "test:unit": "vitest run --coverage",
    "cy:verify": "cypress verify",
    "cy:run-junit-reporter:component": "cypress run -q --component --reporter junit --reporter-options 'mochaFile=cypress/results/component/results-[hash].xml' --config videosFolder=cypress/videos/component screenshotsFolder=cypress/screenshots/e2e",
    "cy:run-junit-reporter:e2e": "cypress run --reporter junit --reporter-options 'mochaFile=cypress/results/e2e/results-[hash].xml' --config videosFolder=cypress/videos/e2e screenshotsFolder=cypress/screenshots/e2e"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.10.8",
    "cross-env": "^7.0.3",
    "cypress-axe": "^1.4.0",
    "msw": "^1.2.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vite-plugin-istanbul": "^4.1.0",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.4",
    "cypress": "^12.7.0",
    "cypress-junit-reporter": "^1.3.1",
    "postcss": "^8.4.12",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vite": "^2.9.16"
  },
  "msw": {
    "workerDirectory": "."
  }
}
