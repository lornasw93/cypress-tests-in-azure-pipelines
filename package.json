{
  "name": "react-vite-ts",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "vite",
    "start:e2e": "cross-env VITE_EHR_SERVER_URL=/ npm run build && npm run serve",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "pretest": "rimraf .nyc_output coverage .reports && mkdir .nyc_output .nyc_output/reports",
    "posttest": "npm run posttest:unit && npm run posttest:component && npm run cover:combine && npm run cover:report",
    "posttest:unit": "cp .reports/vitest/coverage-final.json .nyc_output/reports/from-vitest.json",
    "posttest:component": "cp .reports/cy/coverage-final.json .nyc_output/reports/from-cypress.json",
    "test": "npm run test:unit && npm run test:component",
    "test:e2e": "cypress run -q",
    "test:component": "cypress run -q --component",
    "test:unit": "vitest run --coverage",
    "cy:run-junit-reporter": "cypress run --reporter junit --reporter-options 'mochaFile=cypress/results/results-[hash].xml,toConsole=true'"
  },
  "dependencies": {
    "@cypress/code-coverage": "^3.10.8",
    "msw": "^0.39.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.4",
    "cypress": "^12.7.0",
    "cypress-junit-reporter": "^1.3.1",
    "postcss": "^8.4.12",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "vite": "^2.9.2"
  },
  "msw": {
    "workerDirectory": "."
  }
}
